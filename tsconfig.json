{
  "extends": ["@tsconfig/node20/tsconfig.json", "@vue/tsconfig/tsconfig.dom.json"],
  "compilerOptions": {
    "composite": true,
    "target": "ESNext",
    "jsx": "preserve",
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": { },
    "types": ["node"],
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "skipLibCheck": true
  },
  "include": [
    "**/src/**/*.vue",
    "**/src/**/*.jsx",
    "**/src/**/*.tsx",
    "**/src/**/*.ts",
    "typings.d.ts"
  ]
}
